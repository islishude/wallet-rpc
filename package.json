{
  "name": "wallet-rpc",
  "version": "5.0.3",
  "description": "jsonrpc 2.0 client for crypto-currencies by TypeScript",
  "main": "dist",
  "types": "dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint:fix": "tslint -p tsconfig.json --fix",
    "build": "npm run lint:fix && del-cli dist && tsc",
    "test": "ts-node test/*.ts",
    "prerelease": "npm run build && npm test",
    "release": "git push --all && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/islishude/wallet-rpc"
  },
  "homepage": "https://github.com/islishude/wallet-rpc#readme",
  "bin": {
    "wallet-rpc": "dist/bin/cli.js"
  },
  "keywords": [
    "Bitcoin",
    "Ethereum",
    "USDT",
    "OmniLayer",
    "EOS",
    "jsonrpc"
  ],
  "author": {
    "email": "lishude@foxmail.com",
    "name": "lishude"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.15",
    "del-cli": "^1.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
